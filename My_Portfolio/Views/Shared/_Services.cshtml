@model My_Portfolio.Models.IndexViewModel
@{
    var data = Model.Titles.ElementAt(0);
   
}
<section id="services" class="services">
    <div class="container">

        <div class="section-title">
            <span>@data.ServicesTitle</span>
            <h2>@data.ServicesTitle</h2>
            <p>@data.ServicesAciklama</p>
        </div>

        <div class="row">
           @foreach (var item in Model.Services)
            {
                 <div class="col-md-6 col-lg-3 d-flex align-items-stretch mb-5 mb-lg-0">
                <div class="icon-box">
                    <div class="icon"><i class="bx @item.Icon"></i></div>
                    <h4 class="title baslik">@item.Baslik</h4>
                    <p class="description">@item.Aciklama</p>
                </div>
            </div>

            }
          

        </div>

    </div>
</section>