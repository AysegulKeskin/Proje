            <a href="@data.Link1" class="twitter"><i class="bx bxl-twitter"></i></a>
            <a href="@data.Link2" class="facebook"><i class="bx bxl-facebook"></i></a>
            <a href="@data.Link3" class="instagram"><i class="bx bxl-instagram"></i></a>
